{
  "name": "normative-react",
  "version": "0.0.1",
  "description": "A React starter theme for Normative.",
  "repository": {
    "type": "git",
    "url": "https://github.com/normative/normative-react.git"
  },
  "main": "webpack.config.js",
  "scripts": {
    "clean": "rm -rf dist && mkdir dist && mkdir dist/server",
    "dev": "NODE_ENV=development babel-node ./src/server",
    "start": "node ./dist/server",
    "build": "npm run clean && npm run build-client && npm run build-server",
    "build-server": "NODE_ENV=production babel -d ./dist/server ./src/server -s",
    "build-client": "NODE_ENV=production babel-node ./webpack/build.js",
    "postinstall": "babel-node ./scripts/postInstall.js",
    "test": "mocha --compilers js:babel-register tests/unit/**.js",
    "ftest": "babel-node ./scripts/runFunctionalTests.js",
    "sinit": "selenium-standalone install",
    "lint": "eslint ./src --ext .js,.jsx"
  },
  "keywords": [
    "react",
    "normative",
    "webpack"
  ],
  "author": "Mark Gaucher",
  "license": "ISC",
  "devDependencies": {
    "babel-preset-react-hmre": "^1.1.1",
    "eslint": "^2.7.0",
    "eslint-config-airbnb": "^7.0.0",
    "eslint-plugin-classes": "^0.1.1",
    "eslint-plugin-filenames": "^0.2.0",
    "eslint-plugin-jsx-a11y": "^0.6.2",
    "eslint-plugin-react": "^4.3.0",
    "hapi-webpack-plugin": "^1.3.0",
    "mocha": "^2.4.5",
    "selenium-standalone": "^5.1.0",
    "wdio-mocha-framework": "^0.2.12",
    "webdriverio": "^4.0.5",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0"
  },
  "dependencies": {
    "autoprefixer": "^6.3.6",
    "babel": "^6.5.2",
    "babel-cli": "^6.7.5",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "css-loader": "^0.23.1",
    "doiuse": "^2.3.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "foundation-sites": "^6.2.1",
    "fs-extra": "^0.26.7",
    "glue": "3.2.0",
    "hapi": "^13.3.0",
    "hoek": "3.0.4",
    "html-webpack-plugin": "^2.15.0",
    "inert": "^3.2.0",
    "node-sass": "^3.4.2",
    "postcss-loader": "^0.8.2",
    "postcss-reporter": "^1.3.3",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-router": "^2.1.1",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.1",
    "stylelint": "^5.4.0",
    "stylelint-config-standard": "^5.0.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.15"
  }
}
